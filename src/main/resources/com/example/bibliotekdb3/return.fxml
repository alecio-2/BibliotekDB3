<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="750.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bibliotekdb3.ReturnController">
    <children>
      <Button fx:id="backHome" layoutX="20.0" layoutY="20.0" mnemonicParsing="false" onAction="#backHome" text="Logout" />
      <Button fx:id="backAccount" layoutX="110.0" layoutY="20.0" mnemonicParsing="false" onAction="#backAccount" text="Back to My Account" />
      <Button fx:id="close" layoutX="940.0" layoutY="20.0" mnemonicParsing="false" onAction="#close" text="Close" />
      <Label layoutX="464.0" layoutY="196.0" text="Return object" />
      <Button fx:id="returnButton" layoutX="474.0" layoutY="332.0" mnemonicParsing="false" onAction="#returnArticle" text="Return" />
      <Label layoutX="323.0" layoutY="263.0" text="Article ID" />
      <TextField fx:id="inputField" layoutX="425.0" layoutY="259.0" onAction="#returnArticle" promptText="Write ArtikleNr to return" />
    </children>
</AnchorPane>
