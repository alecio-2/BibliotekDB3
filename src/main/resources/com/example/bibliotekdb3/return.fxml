<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="750.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bibliotekdb3.ReturnController">
    <children>
        <AnchorPane prefHeight="750.0" prefWidth="1000.0">
            <children>
                <Label layoutX="454.0" layoutY="186.0" text="Return article" />
                <Button fx:id="returnButton" layoutX="464.0" layoutY="322.0" mnemonicParsing="false" onAction="#returnArticle" text="Return" />
                <MenuBar layoutX="-6.0" layoutY="2.0">
                    <menus>
                        <Menu mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem fx:id="backHome" mnemonicParsing="false" onAction="#backHome" text="Back to HomePage" />
                                <MenuItem fx:id="backAccount" mnemonicParsing="false" onAction="#backAccount" text="Back to My Account" />
                                <MenuItem fx:id="close" mnemonicParsing="false" onAction="#close" text="Close" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Edit">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Delete" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Help">
                            <items>
                                <MenuItem mnemonicParsing="false" text="About" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            <Label layoutX="313.0" layoutY="253.0" text="Article ID" />
            <TextField fx:id="inputField" layoutX="415.0" layoutY="249.0" onAction="#returnArticle" promptText="Write ArtikleNr to return" />
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
